{
  "name": "www",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fialucci/fialucci-org-website.git"
  },
  "bugs": {
    "url": "https://github.com/fialucci/fialucci-org-website/issues"
  },
  "homepage": "https://fialucci.github.io/",
  "scripts": {
    "dev": "vite --open /",
    "build": "tsc && vite build",
    "build:pages": "npm run build && node scripts/copy404.mjs",
    "preview": "vite preview --open /",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run build:pages",
    "deploy": "node scripts/deploy-gh-pages.mjs"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "dompurify": "^3.3.0",
    "katex": "^0.16.11",
    "marked": "^16.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.2",
    "eslint": "^9.11.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.8",
    "gh-pages": "^6.3.0",
    "jsdom": "24.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.0.0",
    "vitest": "2.1.9",
    "rimraf": "^5.0.5"
  }
}
